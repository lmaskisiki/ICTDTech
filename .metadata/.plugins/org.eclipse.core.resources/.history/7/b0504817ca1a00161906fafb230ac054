package ws.rs;

 import javax.inject.Inject;
import javax.ws.rs.GET;
import javax.ws.rs.Path;
import javax.ws.rs.Produces;

@Path("/hello")
public class Hello implements Ihello {
  @Inject
  
	
	@Path("/sayhello")
	@GET
	@Produces("application/json")
	@Override
	public String saySomething() {
		return "Hello User";
	}
}
