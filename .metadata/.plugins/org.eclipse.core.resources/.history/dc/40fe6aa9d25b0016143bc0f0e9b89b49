package birthtech.services.rest;

import java.util.List;

import javax.ws.rs.GET;
import javax.ws.rs.Path;
import javax.ws.rs.PathParam;

import birthtech.entities.Checkup;
import birthtech.entities.Child;
import birthtech.entities.Maternity;


@Path("/wclinic")
public interface IWclinic {

	@GET
	@Path("/checkup/list/{patientid}")
	public List<Checkup> checkuplist(@PathParam("patientid") String patientId);
	
	@GET
	@Path("/patient/children/{patientid}")
	public List<Child> children(@PathParam("patientid") String patientId);
	
	@GET
	@Path("/patients")
	public List<Maternity> patients();
	
	
	
}
