package useraccount.soap.client;

import javax.xml.ws.WebServiceRef;

import useraccount.soap.services.AccountService;
import useraccount.soap.services.PersonInterface;

public class UAServiceClient {

	@WebServiceRef(wsdlLocation="http://localhost:8080/userGate/AccountService?wsdl")
	private  AccountService service;
	public UAServiceClient() {
		getUserRoles();
	}
	public void getUserRoles(){
		PersonInterface servicePort= service.getAccountServicePort();
		System.out.println(servicePort.isUserMemberOf("admin","Admin"));
	}
	
	 
}
